generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model session {
  id        String   @id
  sid       String   @unique(map: "Session_sid_key")
  data      String
  expiresAt DateTime
}

model users {
  id       Int      @id @default(autoincrement())
  email    String   @unique(map: "Users_email_key")
  username String   @unique(map: "Users_username_key")
  hash     String
  salt     String
  verified Boolean?
}
